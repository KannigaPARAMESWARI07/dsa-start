#include<bits/stdc++.h>
using namespace std;
void even_odd(int arr[],int n){
	int odd=0;
	int even=0;
	for (int i=0;i<n;i++){
		if(arr[i]%2==0){
			even++;
		}
		else{
			odd++;
		}
	}
		cout<<"odd"<<odd<<endl;
      	cout<<"even"<<even<<endl;
		
		
	
	
}
int main(){
	int n;
	cin>>n;
	int arr[n];
	for (int i=0;i<n;i++){
		cin>>arr[i];
	}
	
	even_odd(arr,n);
	
}

