#include<bits/stdc++.h>
using namespace std;
int slar(int arr[],int n){
	int largest=arr[0];
	int slargest=INT_MIN;
	for (int i=0;i<n;i++){
		if(arr[i]>largest){
			slargest=largest;
			largest=arr[i];
		}
		else if(arr[i]<largest && arr[i]>slargest){
			slargest=arr[i];
		}	
		}
	return slargest;		
}
int main(){
	int n;
	cin>>n;
	int arr[n];
	for (int i=0;i<n;i++){
		cin>>arr[i];
	}
	int res;
	res=slar(arr,n);
	cout<<res;	
}

