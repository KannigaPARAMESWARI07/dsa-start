#include<bits/stdc++.h>
using namespace std;
string two(vector<int> &arr,int n,int target){
	int left=0,right=n-1;
	sort(arr.begin(),arr.end());
	for(int i=0;i<n;i++){
		int sum=arr[left]+arr[right];
		if(sum==target){
			return "yes";
		}
		else if(sum<target){
			left++;
		}
		else{
			right--;
		}
		}
		return "no";	
}
int main(){
	int n;
	cin>>n;
	int target;
	cin>>target;
	vector <int> arr(n);
	for(int i=0;i<n;i++){
		cin>>arr[i];
	} 
	
	
	cout<<two(arr,n,target);
}
