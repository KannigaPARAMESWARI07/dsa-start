#include<bits/stdc++.h>
using namespace std;
int two(vector<int> &arr,int n){
	int sum=0;
	int max=0;
	int start=0;
	int ansstart=-1;
	int ansend=-1;
	for(int i=0;i<arr.size();i++){
		if(sum==0){
			start=i;
		}
		sum=sum+arr[i];	
		if(sum > max){
			max=sum;
			ansstart=start;
			ansend=i;
		}
		if(sum<0){
			sum=0;
		}
	}
	 cout << "Subarray: ";
    for(int i = ansstart; i <= ansend; i++){
        cout << arr[i] << " ";
    }
    cout << endl;
		return max;
	
}
int main(){
	int n;
	cin>>n;

	vector <int> arr(n);
	for(int i=0;i<n;i++){
		cin>>arr[i];
	} 
	 cout<<two(arr,n);
    }
	
